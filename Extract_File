file_path = input()

# Find the last occurrence of "/" to locate the file name
last_slash_index = file_path.rfind("/")

# Find the last occurrence of "." to locate the file extension
last_dot_index = file_path.rfind(".")

# Extract the file name and extension using string slicing
file_name = file_path[last_slash_index + 1 : last_dot_index]
file_extension = file_path[last_dot_index + 1 :]

print("File name:", file_name)
print("File extension:", file_extension)
